{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "migrate": "tsx scripts/run-migrations.ts",
    "create:migration": "tsx scripts/create-migration.ts",
    "bootstrap:migrations": "tsx scripts/bootstrap-schema-version.ts",
    "predev": "npm run migrate",
    "prestart": "npm run migrate",
    "start": "vite",
    "dev": "vite",
    "dev:test": "vite --mode test",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "setup:e2e": "node scripts/setup-e2e.js",
    "setup:branch-protection": "node scripts/setup-branch-protection.js",
    "verify:branch-protection": "node scripts/verify-branch-protection.js",
    "test:create-user": "node scripts/create-test-user.js",
    "test:cleanup-users": "node scripts/cleanup-test-users.js --database=test --dry-run",
    "prod:list-users": "node scripts/list-production-users.js",
    "prod:delete-users": "node scripts/delete-invalid-users.js",
    "prod:delete-users:dry-run": "node scripts/delete-invalid-users.js --dry-run",
    "prod:cleanup-users": "node scripts/cleanup-test-users.js --database=production --dry-run",
    "health:check": "node scripts/monitor-email-health.js",
    "test": "node scripts/run-vitest.js"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.57.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.344.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.2.3",
    "react-router-dom": "^7.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.47.0",
    "@testcontainers/postgresql": "^11.7.2",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "testcontainers": "^10.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.7",
    "vitest": "^2.1.4"
  }
}
